<?xml version="1.0" encoding="UTF-8"?>
<recipeManger:RecipeManager
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:recipeManger="platform:/resource/recipeManger/model/RecipeManger.ecore">
  <kitchenUtensil
      Name="Messer"/>
  <kitchenUtensil
      Name="Ofen"/>
  <foodCategory
      Name="Vorspeise"/>
  <foodCategory
      Name="Vegetarisch"/>
  <user xsi:type="recipeManger:Author"
      Email="e@s.com"
      Name="Kim"
      writes="//@user.0/@isOwner.0/@has.0 //@user.0/@isOwner.0/@has.1 //@user.0/@isOwner.0/@has.2">
    <isOwner
        Name="Spagetti"
        foodCategory="//@foodCategory.0 //@foodCategory.1"
        has veganism level="//@veganismLevel.0">
      <has Comment="Ist gut"/>
      <has Comment="Ist so mittel"/>
      <has Comment="Ist echt mies"/>
      <hasIngredients
          Name="Milch"
          has veganism level="//@veganismLevel.1"/>
      <hasIngredients
          Name="Salz"
          has veganism level="//@veganismLevel.2"/>
      <hasIngredients
          Name="Zucker"
          has veganism level="//@veganismLevel.0"/>
    </isOwner>
    <isOwner
        Name="Pudding"
        foodCategory="//@foodCategory.1"
        hasRecipes="//@user.0/@isOwner.0 //@user.0/@isOwner.1"
        has veganism level="//@veganismLevel.0">
      <has Comment="War mies"/>
    </isOwner>
  </user>
  <user xsi:type="recipeManger:Author"
      calls="//@user.0/@isOwner.0"
      Email="n@g.com"
      Name="Lukas"
      writes="//@user.0/@isOwner.0/@has.0 //@user.0/@isOwner.0/@has.1"/>
  <user calls="//@user.0/@isOwner.0"/>
  <user xsi:type="recipeManger:Author"
      Email="n@k.com"
      Name="Michel">
    <isOwner
        Name="Grießbrei"
        Duration="130"
        Difficulty="8"
        foodCategory="//@foodCategory.1"
        has veganism level="//@veganismLevel.0"/>
    <isOwner
        Name="Klöße"
        has veganism level="//@veganismLevel.2"/>
    <isOwner
        Name="Sauce Hollondaise"
        requiredUtilities="//@kitchenUtensil.0 //@kitchenUtensil.1"
        foodCategory="//@foodCategory.0"
        hasRecipes="//@user.0/@isOwner.0"
        has veganism level="//@veganismLevel.1"/>
  </user>
  <user calls="//@user.0/@isOwner.1"/>
  <veganismLevel
      xsi:type="recipeManger:Vegan"/>
  <veganismLevel
      xsi:type="recipeManger:Carnivorous"/>
  <veganismLevel
      xsi:type="recipeManger:Vegetaric"/>
</recipeManger:RecipeManager>
